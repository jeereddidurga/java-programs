 class ArthameticOperations{
    public static void main(String[] args) {
        int a,b,c,d,e,f,g;
        a=10;
        b=5;
        c=a-b;
        d=a*b;
        e=a%b;
        f=a/b;
        g=a+b;
        System.out.println(c);
        System.out.println(d);
        System.out.println(e);
        System.out.println(f);
        System.out.println(g);
    }
}