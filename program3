import java.util.Scanner;
class ForLoop {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("enter the number n");
        int n = scanner.nextInt();
        int i = scanner.nextInt();
        int sum = 0;
         for(i=0;i<n;i++){
             sum = sum + i;
             System.out.println(sum);
         }
        
        
    }
}